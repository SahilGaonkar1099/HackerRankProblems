#You are given a string and your task is to swap cases. In other words, convert all lowercase letters to uppercase letters and vice versa.
def swap_case(s):
    j=""
    for i in s:
        if i.islower():
            j+=i.upper()
        elif i.isupper():
            j+=i.lower()
        else:
            j+=i    
    return j        


if __name__ == '__main__':
    s = input()
    result = swap_case(s)
    print(result)
